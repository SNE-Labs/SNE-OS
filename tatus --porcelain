[32m+[m[32m## ğŸ¯ **OPÃ‡ÃƒO 2: Deploy Passo a Passo**[m
[32m+[m
[32m+[m[32m### **1. Preparar Infraestrutura**[m
[32m+[m
[32m+[m[32m```bash[m
[32m+[m[32m# Configurar projeto e regiÃ£o[m
[32m+[m[32mexport PROJECT_ID=sne-v1[m
[32m+[m[32mexport REGION=us-central1[m
[32m+[m[32mgcloud config set project $PROJECT_ID[m
[32m+[m
[32m+[m[32m# Criar Cloud SQL[m
[32m+[m[32mgcloud sql instances create sne-db-prod \[m
[32m+[m[32m  --database-version=POSTGRES_15 \[m
[32m+[m[32m  --cpu=2 \[m
[32m+[m[32m  --memory=4GB \[m
[32m+[m[32m  --region=$REGION \[m
[32m+[m[32m  --root-password="5a9862d483ba291dc2012f254cce03a7"[m
[32m+[m
[32m+[m[32mgcloud sql databases create sne --instance=sne-db-prod[m
[32m+[m[32mgcloud sql users create sne_admin \[m
[32m+[m[32m  --instance=sne-db-prod \[m
[32m+[m[32m  --password="5a9862d483ba291dc2012f254cce03a7"[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### **2. Inicializar Banco**[m
[32m+[m
[32m+[m[32m```bash[m
[32m+[m[32m# Executar script de inicializaÃ§Ã£o[m
[32m+[m[32mcd backend-v2/services/sne-web[m
[32m+[m[32mpython init_db.py[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### **3. Deploy Backend**[m
[32m+[m
[32m+[m[32m```bash[m
[32m+[m[32m# Build e deploy[m
[32m+[m[32mdocker build -t gcr.io/$PROJECT_ID/sne-web:latest .[m
[32m+[m[32mdocker push gcr.io/$PROJECT_ID/sne-web:latest[m
[32m+[m
[32m+[m[32mgcloud run deploy sne-web \[m
[32m+[m[32m  --image gcr.io/$PROJECT_ID/sne-web:latest \[m
[32m+[m[32m  --platform managed \[m
[32m+[m[32m  --region=$REGION \[m
[32m+[m[32m  --allow-unauthenticated \[m
[32m+[m[32m  --set-env-vars="SECRET_KEY=sne-jwt-secret,SIWE_DOMAIN=radar.snelabs.space,SIWE_ORIGIN=https://radar.snelabs.space" \[m
[32m+[m[32m  --set-secrets="DB_PASSWORD=sne-db-password:latest,JWT_SECRET=sne-jwt-secret:latest" \[m
[32m+[m[32m  --add-cloudsql-instances=$PROJECT_ID:$REGION:sne-db-prod[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m---[m
